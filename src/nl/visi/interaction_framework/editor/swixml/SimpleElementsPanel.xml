<?xml version="1.0" encoding="UTF-8"?>
<!-- This XML file is used to create a Swing container using the SwiXml rendering engine. org.jvnet.substance.SubstanceLookAndFeel 
	com.jgoodies.looks.plastic.PlasticXPLookAndFeel org.jdesktop.swingx.plaf.nimbus.NimbusLookAndFeel a03.swing.plaf/A03LookAndFeel -->
<panel bundle="nl.visi.interaction_framework.editor.locale.Editor" id="simpleElementsPanel"
	plaf="com.jgoodies.looks.plastic.PlasticXPLookAndFeel" layout="BorderLayout">
	<splitpane constraints="BorderLayout.CENTER" ContinuousLayout="true" DividerLocation="220"
		OneTouchExpandable="true" Orientation="JSplitPane.VERTICAL_SPLIT">
		<panel Border="EmptyBorder(4,4,4,4)" Layout="GridBagLayout">
			<scrollpane>
				<gridbagconstraints Fill="GridBagConstraints.BOTH" Gridx="0" Gridy="0"
					Weightx="1.0" Weighty="1.0" />
				<table id="tbl_Elements" SelectionMode="ListSelectionModel.SINGLE_SELECTION" />
			</scrollpane>
			<panel Layout="GridBagLayout">
				<gridbagconstraints Fill="GridBagConstraints.HORIZONTAL" Gridx="0" Gridy="1"
					Weightx="1.0" />
				<button id="btn_NewElement" Action="newElement" Text="lbl_NewSimpleElement">
					<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="0"
						Insets="2,2,2,2" />
				</button>
				<button id="btn_DeleteElement" Action="deleteElement" Enabled="false" Text="lbl_Remove">
					<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="1" Gridy="0"
						Insets="2,0,2,2" Weightx="1.0" />
				</button>
			</panel>
		</panel>
		<splitpane ContinuousLayout="true" DividerLocation="280" OneTouchExpandable="true"
			Orientation="JSplitPane.HORIZONTAL_SPLIT">
			<scrollpane Border="TitledBorder(lbl_Attributes)">
				<panel Border="EmptyBorder(4,4,4,4)" Layout="GridBagLayout">
					<label Text="lbl_Id">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="0"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_Id" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="0" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_Description">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="1"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_Description" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="1" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_InterfaceType">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="2"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_InterfaceType" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="2" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_State">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="3"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_State" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="3" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_Language">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="4"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_Language" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="4" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_Category">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="5"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_Category" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="5" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_HelpInfo">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="6"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_HelpInfo" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="6" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<label Text="lbl_ValueList">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="7"
							Insets="2,2,2,2" />
					</label>
					<textfield id="tfd_ValueList" enabled="false" Columns="8">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Fill="GridBagConstraints.HORIZONTAL"
							Gridx="1" Gridy="7" Insets="2,2,2,2" Weightx="1.0" />
					</textfield>
					<panel>
						<gridbagconstraints Fill="GridBagConstraints.BOTH" Gridx="0" Gridy="8"
							GridWidth="2" Weighty="1.0" />
					</panel>
				</panel>
			</scrollpane>
			<scrollpane Border="TitledBorder(lbl_Relations)">
				<panel Border="EmptyBorder(4,4,4,4)" Layout="GridBagLayout">
					<label Text="lbl_UserDefinedType">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="0" Gridy="0"
							Insets="2,2,2,2" />
					</label>
					<combobox id="cbx_UserDefinedType" Action="setUserDefinedType">
						<gridbagconstraints Fill="GridBagConstraints.HORIZONTAL" Gridx="1" Gridy="0"
							Insets="2,2,2,2" Weightx="1.0" />
					</combobox>
					<button id="btn_NavigateUserDefinedType" Action="navigateUserDefinedType" Enabled="false"
						Icon="img_ForwardNav">
						<gridbagconstraints Anchor="GridBagConstraints.WEST" Gridx="2" Gridy="0"
							Insets="2,2,2,2" />
					</button>
					<panel>
						<gridbagconstraints Fill="GridBagConstraints.BOTH" Gridwidth="3" Gridx="0"
							Gridy="1" Weightx="1.0" Weighty="1.0" />
					</panel>
				</panel>
			</scrollpane>
		</splitpane>
	</splitpane>
</panel>