<dialog id="transactionCopyDialog" Size="480,360"
	Title="lbl_TransactionCopyDialog" Modal="true"
	DefaultCloseOperation="JFrame.EXIT_ON_CLOSE"
	bundle="nl.visi.interaction_framework.editor.locale.Editor"
	plaf="com.jgoodies.looks.plastic.PlasticXPLookAndFeel">
	<panel layout="GridBagLayout" border="EmptyBorder(8,8,8,8)">
		<panel Layout="GridBagLayout" Border="TitledBorder(lbl_Copy)">
			<gridbagconstraints Fill="GridBagConstraints.BOTH"
				Gridx="0" Gridy="0" Weightx="1.0" />
			<buttongroup id="copyAction">
				<radiobutton id="rdb_ShallowCopy" text="lbl_ShallowCopy"
					Selected="true" FocusPainted="false">
					<gridbagconstraints
						Fill="GridBagConstraints.HORIZONTAL" Gridx="0" Gridy="0"
						Insets="4,4,4,4" Weightx="1.0" />
				</radiobutton>
				<radiobutton id="rdb_DeepInternalOnlyCopy"
					text="lbl_DeepInternalOnlyCopy" Selected="false"
					FocusPainted="false">
					<gridbagconstraints
						Fill="GridBagConstraints.HORIZONTAL" Gridx="0" Gridy="1"
						Insets="4,4,4,4" Weightx="1.0" />
				</radiobutton>
				<radiobutton id="rdb_DeepCopy" text="lbl_DeepCopy"
					Selected="false" FocusPainted="false">
					<gridbagconstraints
						Fill="GridBagConstraints.HORIZONTAL" Gridx="0" Gridy="2"
						Insets="4,4,4,4" Weightx="1.0" />
				</radiobutton>
			</buttongroup>
		</panel>
